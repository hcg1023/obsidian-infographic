{
	"name": "obsidian-infographic",
	"version": "1.0.1",
	"description": "Render infographic diagrams in Obsidian using @antv/infographic",
	"main": "main.js",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/hcg1023/obsidian-infographic.git"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
	},
	"keywords": ["obsidian", "plugin", "infographic", "diagram"],
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/node": "^16.11.6",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1"
	},
	"dependencies": {
		"@antv/infographic": "^0.2.7",
		"obsidian": "latest"
	}
}
